<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions targetNamespace="http://www.bluetechnology.com.tw/wsdl/v1-0"
	xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:bt="http://www.bluetechnology.com.tw" xmlns:btwsdl="http://www.bluetechnology.com.tw/wsdl/v1-0"
	xmlns:sec="http://cxf.apache.org/configuration/security"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.bluetechnology.com.tw"
				schemaLocation="TicketSeatInquiry.xsd" />
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="ticketSeatInquiryResultRequest">
		<wsdl:part name="body" element="bt:ticketSeatInquiryResultRequest" />
	</wsdl:message>
	<wsdl:message name="ticketSeatInquiryResultResponse">
		<wsdl:part name="body" element="bt:ticketSeatInquiryResultResponse" />
	</wsdl:message>

	<wsdl:portType name="TicketSeatInquiryPortType">
		<wsdl:operation name="ticketSeatInquiryResult">
			<wsdl:input message="btwsdl:ticketSeatInquiryResultRequest" />
			<wsdl:output message="btwsdl:ticketSeatInquiryResultResponse" />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="TicketSeatInquiryHttpBinding" type="btwsdl:TicketSeatInquiryPortType">
		<wsdlsoap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
			
		<wsdl:operation name="ticketSeatInquiryResult">
			<wsdlsoap:operation soapAction="" />
			<wsdl:input>
				<wsdlsoap:body parts="body" use="literal" />
			</wsdl:input>
			<wsdl:output>
				<wsdlsoap:body parts="body" use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<!-- 從這裡為起點 -->
	<wsdl:service name="TicketSeatInquiryHttpService">
		<wsdl:port binding="btwsdl:TicketSeatInquiryHttpBinding" name="TicketSeatInquiryHttpPort"> <!-- 找28行的binding -->
			<wsdlsoap:address
				location="http://127.0.0.1:8089/webservice-ticket-service/TicketSeatInquiryHttpService" /><!-- 
				這是要呼叫的網址 -->
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>